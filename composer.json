{
    "name": "example/graphql",
    "type": "typo3-cms-extension",
    "description": "Access TYPO3 CMS through GraphQL over HTTP.",
    "license": ["GPL-2.0-or-later"],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/typo3-initiatives/graphql"
        }
    ],
    "require": {
        "typo3/cms-graphql": "10.0.*@dev",
        "typo3/cms-extbase": "10.0.*@dev"
    },
    "replace": {
        "example/graphql": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Example\\": "Classes/"
        }
    }
}